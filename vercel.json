{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/games",
            "dest": "api/index.js"
        },
        {
            "src": "/",
            "dest": "api/index.js"
        },
        {
            "src": "/visit-count",
            "dest": "/api/index.js"
        },
        {
            "src": "/visit-count/increment-count",
            "dest": "/api/index.js"
        }
    ]
}